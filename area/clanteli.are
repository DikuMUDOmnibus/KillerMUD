  $_rand = int_random 1 4
  if numeval $_rand == 1
  and !hasflag $r blessed
    cast bless $r
    mob setflag $r blessed 5
  endif
  if numeval $_rand == 2
  and !hasflag $r lucked
    cast luck $r
    mob setflag $r lucked 5
  endif
  if numeval $_rand == 3
  and !hasflag $r cured_ser
  and hpcnt $r < 75
    cast 'cure ser' $r
    mob setflag $r cured_ser 5
  endif
  if numeval $_rand == 4
  and !hasflag $r cured_cri
  and hpcnt $r < 40
    cast 'cure cri' $r
    mob setflag $r cured_cri 5
  endif
endif
